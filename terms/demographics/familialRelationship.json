{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "sage.annotations-demographics.familialRelationship-0.0.2",
    "type": "string",
    "description": "Family relationship between individuals in the study",
    "anyOf": [
        {
            "const": "daughter",
            "description": "A female human offspring",
            "source": "https://ncim.nci.nih.gov/ncimbrowser/ConceptReport.jsp?dictionary=NCI%20Metathesaurus&code=C0337552"
        },
        {
            "const": "father",
            "description": "A male parent",
            "source": "https://ncim.nci.nih.gov/ncimbrowser/ConceptReport.jsp?dictionary=NCI%20Metathesaurus&code=C0015671"
        },
        {
            "const": "mother",
            "description": "A female parent",
            "source": "https://ncim.nci.nih.gov/ncimbrowser/ConceptReport.jsp?dictionary=NCI%20Metathesaurus&code=C0026591"
        },
        {
            "const": "offspring",
            "description": "A first-generation, biological descendant of any age",
